{
  "name": "pipapp",
  "version": "0.1.0",
  "description": "Show the external display screen in a window.",
  "license": "MIT",
  "author": {
    "name": "Takeshi Katano"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tksh164/pipapp.git"
  },
  "productName": "Displayscope",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "electron:pretask": "copyfiles -f ../setmousecursorpos/setmousecursorpos.exe build",
    "electron:build": "yarn run electron:pretask && vue-cli-service electron:build",
    "electron:serve": "yarn run electron:pretask && vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "bl": "1.2.3",
    "element-ui": "^2.4.5",
    "minimist": "1.2.3",
    "node-forge": "0.10.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-typescript": "~4.2.0",
    "@vue/cli-plugin-unit-jest": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "1.0.0-beta.31",
    "electron": "9.3.5",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "prettier": "^1.19.1",
    "typescript": "~3.7.5",
    "vue-cli-plugin-electron-builder": "~1.4.5",
    "vue-cli-plugin-element": "~1.0.1",
    "vue-template-compiler": "^2.6.11",
    "copyfiles": "^2.4.1"
  }
}
